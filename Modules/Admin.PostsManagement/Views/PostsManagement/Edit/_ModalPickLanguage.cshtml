<style>
    .img-logo-flag {
        height: 20px;
        width: 20px;
    }
</style>
<div class="modal fade" id="pickLanguageModal" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered1 modal-simple modal-add-new-cc">
        <div class="modal-content p-3 p-md-5">
            <div class="modal-body">
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                <div class="text-center mb-4">
                    <h3>Quản lý bản dịch</h3>
                    @*<p>Add new card to complete payment</p>*@
                </div>
                <div id="pickLanguageForm" class="row g-3" onsubmit="return false">
                    <div class="col-8">

                        @*<label class="form-label w-100" for="modalAddCard">Card Number</label>*@

                    </div>
                    <div class="col-4">
                        <button type="submit" class="btn btn-primary me-sm-3 me-1 mt-3">Thêm</button>
                    </div>
                    <div class="col-12 text-center">
                        <button type="submit" class="btn btn-primary me-sm-3 me-1 mt-3">Bài viết</button>
                        <button type="reset" class="btn btn-label-secondary btn-reset mt-3" data-bs-dismiss="modal" aria-label="Close">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<style>
    #template-customizer .template-customizer-open-btn::before {
        background-image: url('data:image/png;base64,') !important;
    }
</style>

<div id="template-customizer" class="invert-bg-white template-customizer" style="visibility: visible;">
    <a href="javascript:OpenConfigModel()" class="template-customizer-open-btn" tabindex="-1"></a> <div class="p-4 m-0 lh-1 border-bottom template-customizer-header position-relative py-3">
        <h4 class="template-customizer-t-panel_header mb-2">Quản lý đa ngôn ngữ</h4>
        @*<p class="template-customizer-t-panel_sub_header mb-0">Customize and preview in real time</p>*@
        <div class="d-flex align-items-center gap-2 position-absolute end-0 top-0 mt-4 me-3">
            <a href="javascript:void(0)" class="template-customizer-reset-btn text-body" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Reset Customizer">
                <i class="bx bx-refresh fs-4"></i>
                <span class="badge rounded-pill bg-danger badge-dot badge-notifications"></span>
            </a>
            <a href="javascript:void(0)" onclick="CloseConfigModel()" class="template-customizer-close-btn fw-light text-body" tabindex="-1">
                <i class="bx bx-x fs-4"></i>
            </a>
        </div>
    </div> <div class="template-customizer-inner">

        <div class="template-customizer-layout">
            <div class="card">

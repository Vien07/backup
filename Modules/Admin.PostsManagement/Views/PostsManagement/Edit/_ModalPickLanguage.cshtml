                <div class="card-body">
                    <div class="tab-content p-0">
                        <div class="tab-pane fade active show" id="navs-pills-within-card-adminconfig" role="tabpanel">
                            <form id="Admin_PostsManagementConfig" class="form form-vertical mt-3">
                                <div class="form-body">
                                    <div class="row">

                                        <div class="col-6">
                                            @{

                                                List<SelectControlData> aa = new List<SelectControlData>();
                                                aa.Add(new SelectControlData { Name = "<img class=\"img-logo-flag\" src=\"" + @Url.Content(LocalizedContentPropertyConstants.ModuleInfo.PathAssets) + "/flags/vietnam.png\" /> Việt Nam", Value = "vi" });
                                                aa.Add(new SelectControlData { Name = "<img class=\"img-logo-flag\" src=\"" + @Url.Content(LocalizedContentPropertyConstants.ModuleInfo.PathAssets) + "/flags/united-kingdom.png\" /> English", Value = "en" });
                                                aa.Add(new SelectControlData { Name = "<img class=\"img-logo-flag\" src=\"" + @Url.Content(LocalizedContentPropertyConstants.ModuleInfo.PathAssets) + "/flags/china.png\" /> Chinese", Value = "cn" });
                                                aa.Add(new SelectControlData { Name = "<img class=\"img-logo-flag\" src=\"" + @Url.Content(LocalizedContentPropertyConstants.ModuleInfo.PathAssets) + "/flags/south-korea.png\" /> Korean", Value = "kr" });
                                                aa.Add(new SelectControlData { Name = "<img class=\"img-logo-flag\" src=\"" + @Url.Content(LocalizedContentPropertyConstants.ModuleInfo.PathAssets) + "/flags/japan.png\" /> Japanese", Value = "jp" });
                                                NiceSelectModel selectModel2 = new NiceSelectModel();
                                                selectModel2.Data = aa;
                                                selectModel2.Id = "LangCode";
                                                selectModel2.Size = "";
                                                selectModel2.DefaultValue = "vi";
                                                selectModel2.DefaultText = "Việt Nam";
                                                selectModel2.Class = "nice-select";
                                            }
                                            @await Component.InvokeAsync(nameof(ComponentUILibrary.ViewComponents.NiceSelectComponent), new { model = selectModel2 })
                                        </div>
                                        <div class="col-4">
                                            <button type="submit" class="btn btn-primary me-sm-3">Thêm</button>
                                        </div>


                                    </div>
                                    <div>
                                        @{
                                            ComponentUILibrary.Models.TableData table = new ComponentUILibrary.Models.TableData();
                                            table.ID = "test2";
                                            table.Class = "table table-hover bg-white mb-0 ";
                                            table.Body = "";

                                            List<TableHead> tableHead = new List<TableHead>();
                                            tableHead.Add(new TableHead { Name = "#", Width = 1, Filter = "ASC", OrderBy = "Order" });
                                            tableHead.Add(new TableHead { Name = "Language", Width = 1 });
                                            tableHead.Add(new TableHead { Name = "Action", Width = 100 });
                                            table.Header = tableHead;
                                        }
                                        @await Component.InvokeAsync(nameof(ComponentUILibrary.ViewComponents.TableComponent), new { table = table })
                                    </div>
                                    <div class="col-12 d-flex justify-content-end mt-2">
                                        <button type="button" onclick="_action.SaveConfig('Admin_PostsManagementConfig','Admin')" class="btn btn-primary me-1 mb-1">Cập nhật</button>
                                        @*                                        <button type="reset" class="btn btn-light-secondary me-1 mb-1">Default</button>
                                        *@
                                    </div>
                                </div>
                            </form>
                        </div>

<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<ErrorOnDuplicatePublishOutputFiles>false</ErrorOnDuplicatePublishOutputFiles>

		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>
		<ChecksumAlgorithm>SHA1</ChecksumAlgorithm>

	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="wwwroot\category\**" />
	  <Compile Remove="wwwroot\course\**" />
	  <Content Remove="wwwroot\category\**" />
	  <Content Remove="wwwroot\course\**" />
	  <EmbeddedResource Remove="wwwroot\category\**" />
